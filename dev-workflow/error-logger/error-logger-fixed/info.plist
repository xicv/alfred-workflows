                <key>script</key>
                <string>#!/bin/bash

query="{query}"
LOG_DIR="$HOME/Desktop/dev-logs"
mkdir -p "$LOG_DIR"

filename="error_$(date +%Y%m%d_%H%M%S).log"
filepath="$LOG_DIR/$filename"

echo "Error: $query" > "$filepath"
echo "Time: $(date)" >> "$filepath"
echo "Project: $(pwd | xargs basename)" >> "$filepath"
echo "" >> "$filepath"
pbpaste >> "$filepath"

echo -n "$filepath" | pbcopy
osascript -e "display notification \"Error log created: $filename\" with title \"Dev Error Logger\""
echo "Error log created: $filename"</string>
