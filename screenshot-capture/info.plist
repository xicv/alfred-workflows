                <key>script</key>
                <string>#!/bin/bash

query="{query}"

case "$query" in
    "fs")
        filename="Screenshot_$(date +%Y%m%d_%H%M%S).png"
        mkdir -p ~/Desktop/dev-screenshots
        screencapture -x ~/Desktop/dev-screenshots/"$filename"
        echo -n "$filename" | pbcopy
        ;;
    "w")
        filename="Screenshot_$(date +%Y%m%d_%H%M%S).png"
        mkdir -p ~/Desktop/dev-screenshots
        screencapture -i -W ~/Desktop/dev-screenshots/"$filename"
        echo -n "$filename" | pbcopy
        ;;
    "s")
        filename="Screenshot_$(date +%Y%m%d_%H%M%S).png"
        mkdir -p ~/Desktop/dev-screenshots
        screencapture -i -s ~/Desktop/dev-screenshots/"$filename"
        echo -n "$filename" | pbcopy
        ;;
    *)
        echo "Usage: sc [fs|w|s]"
        ;;
esac</string>
